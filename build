gcc -c -std=c99 -D_POSIX_C_SOURCE=200112L -DPIC -Wall -Wextra -g -O2 -fpic `pkg-config --cflags alsa` aeq.c
gcc -shared `pkg-config --libs alsa` -Wl,-soname -Wl,libasound_module_pcm_aeq.so -o libasound_module_pcm_aeq.so aeq.o
gcc -std=c99 -D_POSIX_C_SOURCE=200112L -Wall -Wextra -g -O2 `pkg-config --cflags --libs gtk+-2.0` -o aeq gui.c
